(setq sing-to-me-song '(1 4168))(setq long-night-song '(5885 10972))(setq young-song '(15840 19057))(setq rock-song '(22014 25472))(defun read-movie(fn)	(prog (fp lp)		(setq fp (fopen fn))		(setq lp (fread fp))		(fclose fp)		(print (car lp))		(return (cdr lp))	))(setq fruit (read-movie 'oot:movies:fruit))(setq day (read-movie 'oot:movies:day))(setq night (read-movie 'oot:movies:night))(setq words (read-movie 'oot:movies:words))(defun m1()	(while (not (buttonp))		(mov1 () )	))(defun m1a()	(while (not (buttonp))		(mov1a () )	))(defun m2()	(while (not (buttonp))		(mov2 () 60 )	))(defun m2a()	(while (not (buttonp))		(mov2a () )	))(defun m2b()	(while (not (buttonp))		(mov2b () )	))(defun m3()	(while (not (buttonp))		(mov3 long-night-song 0 fruit day night words)		(mov3 young-song -3 fruit day night words)		(mov3 sing-to-me-song -2 fruit day night words)		(mov3 rock-song 1 fruit day night words)	))(defun mm1()	(while (not (buttonp))		(mov1 sing-to-me-song)		(mov1 long-night-song)		(mov1 young-song)		(mov1 rock-song)	))(defun mm2()	(while (not (buttonp))		(mov2 sing-to-me-song 60)		(mov2 long-night-song 60)		(mov2 young-song 60)		(mov2 rock-song 30)	))