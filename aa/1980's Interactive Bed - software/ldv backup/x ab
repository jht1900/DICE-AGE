(defun go()	(mreset)	(mnlines mnlines)	(mnfields mnfields)	(mballoc nbag)		(setq mag-wino (wino mag-def 'mag 200 154 0 40))	(if (nump h-offset) 		(set-val mag-wino 0 h-offset)	)	(if (nump v-offset) 		(set-val mag-wino 1 v-offset)	)		(setq menu-list (cons mag-wino () ))		(setq highcut-wino (wino highcut-def 'high 16 100 202 40))	(if (nump highcut-offset)		(set-val highcut-wino 0 highcut-offset)	)		(setq menu-list (cons highcut-wino menu-list))		(setq mbag-wino (wino mbag-def 'mbag 256 118 220 40))	(setq menu-list (cons mbag-wino menu-list))			(sh))(if (not (nump mnlines))	(setq mnlines 192))(if (not (nump mnfields))	(setq mnfields 42))				(defun reduce()	(setq mnlines 72)	(mnlines mnlines)		(setq mnfields 16)	(mnfields mnfields))(defun full()	(setq mnlines 192)	(mnlines mnlines)		(setq mnfields 42)	(mnfields mnfields)		(moffset 1)	(mstart 12))(defun sh()	(show menu-list 'exec-loop))(defun exec-loop())(setq highcut-def	'(	(vslider 256 highcut-update)	))(defun highcut-update(wino val)	(setq highcut-offset val)	(mhigh val)	(mgrab)	(mget mag-wino))(setq mag-def	'(	(hslider 63 hor-update)		(vslider 256 vert-update)	))(defun hor-update(wino val)	(setq h-offset val)	(moffset val)	(if wino		(progn			(mgrab)			(mget mag-wino)		)	))(defun vert-update(wino val)	(setq v-offset val)	(mstart val)	(if wino		(progn			(mgrab)			(mget mag-wino)		)	))(setq mbag-def	'(			(button grab	mbgrab			0)		(button show	show-all		0)		(button first	go-first		0)		(button	next	go-next			0)		(button	prev	go-prev			0)	))(defun show-all()	(mbshowall mag-wino))(setq mbag-pos 0)(defun go-first()	(setq mbag-pos 0)	(show-n mbag-pos mbag-wino))(defun go-next()	(setq mbag-pos (+ mbagpos 1))	(show-n mbag-pos mbag-wino))(defun go-prev()	(setq mbag-pos (- mbagpos 1))	(show-n mbag-pos mag-wino))(defun show-n(n w)	(mbshow n w))(defun mver()	(mwrite $a6)	(mread))(setq nbag 5)