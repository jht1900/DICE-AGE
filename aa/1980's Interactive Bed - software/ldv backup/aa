(setq sing-to-me-song '(1 4168))(setq long-night-song '(5885 10972))(setq young-song '(15840 19057))(setq rock-song '(22014 25472))(setq long-night-mono-song '(36821 41851))(defun read-movie(fn)	(prog (fp lp)		(setq fp (fopen fn))		(setq lp (fread fp))		(fclose fp)		(print (car lp))		(return (cdr lp))	))(setq words (read-movie 'oot:movies:words))(setq night (read-movie 'oot:movies:night))(setq bed (read-movie 'oot:movies:xbed ))(setq dance (read-movie 'oot:movies:dance))(setq first30 (read-movie 'oot:movies:first30))(setq hands (read-movie 'oot:movies:hands))(setq tender (read-movie 'oot:movies:tender))(setq window (read-movie 'oot:movies:window))(setq fruit (read-movie 'oot:movies:fruit))(setq city (read-movie 'oot:movies:city))(setq day (read-movie 'oot:movies:day))(setq aud2 (read-movie 'oot:movies:audclips2))(setq aud3 (read-movie 'oot:movies:audclips3))