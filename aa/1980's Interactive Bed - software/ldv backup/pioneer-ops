(setq pioneer-cmds	'(		(sa SetAudio		$A0 arg)		(as AutoStop		$F3 arg)		(df DisplayFrame	$F1 arg)		(p  Play			$FD)		(se Search			$F7 arg)		(st SearchRetry		$01 arg)		(ss SlowSpeed		$ED arg)		(fs FastSpeed		$EC arg)		(sf SearchFwd		$F2 arg)		(sr SearchRev		$FA arg)		(xx	Stop			$FB)		(vo SetVideoOn		$1B)		(vf SetVideoOff		$1C)		(f 	StepFwd			$F6)		(r 	StepRev			$FE)		(ff ScanFwd			$F0)		(rr ScanRev			$F8)		(bo BinOutOn		$E7)		(bf BinOutOff		$E5)		(gf GimmeFrame		$D3)		(gs GimmeStatus		$D4)		(cge CharGenOn		$E0)		(cgo CharGenOff		$E1)		(ej Eject			$F9)	))(defun setup(x)	(mapcan 'setup1 x))(defun setup1(x)	(set (cadr x) (caddr x)	)	(eval		(list 			'defun 			(cadr x)			(if (cdddr x)				'(a)				'()			)			(list 'vd  (cadr x) 'a)		)	)	(list (car x)))(defun ? ()	(mapcan 'print			pioneer-cmds))(defun go () 	(setup pioneer-cmds))(go)