(defun m3()	(while (not (buttonp))		(mov3 long-night-song 	0 	words day night fruit)		(mov3 young-song 		1	hands city night fruit)		(mov3 sing-to-me-song 	-2 	day city dance words)		(mov3 rock-song 		1 	tender city first30 night)		(mov3 long-night-song 	-3 	window night hands words)		(mov3 young-song 		1 	window hight fruit hands)		(mov3 sing-to-me-song 	0 	day night hands window)		(mov3 rock-song 		2 	tender night dance city)	))(defun m1()	(vsearch 1)	(while (not (buttonp))		(setaud 3)		(mov1 () )		(mov1 () )		(mov1 () )		(mov1 () )		(setaud 0)	))(defun m1a()	(vsearch 1)	(while (not (buttonp))		(setaud 3)		(mov1a () )		(mov1a () )		(mov1a () )		(mov1a () )		(setaud 0)	))(defun m2()	(vsearch 1)	(while (not (buttonp))		(setaud 3)		(mov2 () 60)		(mov2 () 60)		(mov2 () 60)		(mov2 () 30)		(setaud 0)	))