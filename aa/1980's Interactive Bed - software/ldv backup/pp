(setq picture-piano	'(		;; (top-menu 		;;	34175 34176)		;; (menu-1 		;; 	31191 31192)		;; (menu-2 		;; 	31617 31618)		;; (credits-1 		;;	31171 31172)		(1.c 			31195 31254)		(1.d 			31255 31314)		(1.e			31315 31374)		(1.f			31375 31434)		(1.g			31435 31494)		(1.a			31495 31554)		(1.b			31555 31614)		(2.c			31621 31680)		(2.d			31681 31740)		(2.e			31741 31800)		(2.f			31801 31860)		(2.g			31861 31920)		(2.a			31921 31980)		(2.b			31981 32040)		(3.c			31621 31680)		(3.d			31681 31740)		(3.e			31741 31800)		(3.f			31801 31860)		(3.g			31861 31920)		(3.a			31921 31980)		(3.b			31981 32040)		(4.c			32473 32532)		(4.d			32533 32592)		(4.e			32593 32652)		(4.f			32653 32712)		(4.g			32713 32772)		(4.a			32773 32832)		(4.b			32833 32892)		(5.c			32901 32960)		(5.d			32961 33020)		(5.e			33021 33080)		(5.f			33081 33140)		(5.g			33141 33220)		(5.a			33201 33260)		(5.b			33261 33320)		(6.c			33327 33386)		(6.d			33387 33446)		(6.e			33447 33506)		(6.f			33507 33566)		(6.g			33567 33626)		(6.a			33627 33686)		(6.b			33687 33746)		(7.c			33753 33812)		(7.d			33813 33872)		(7.e			33873 33932)		(7.f			33933 33992)		(7.g			33993 34052)		(7.a			34053 34112)		(7.b			34113 34172)				))(defun install-clip(a)	(put (car a) 'clip (cdr a)	)	(list (car a)))(setq clips	(mapcan 'install-clip picture-piano))(defun play-rand()	(prog (n i w c)		(setq n (length clips))		(setq i 0)		(while (not (buttonp))			(setq i (mod (random) n))			(setq c (nth i clips))			(play-clip c)			(setq w (mod (random) 2))			(if (= w 0)				(play-clip c)			)		)	))(defun play-clip(a)	(play-clip2 (get a 'clip)))(defun play-clip2(c)	(if (listp c)		(progn 			(CharGenOff 0)			(Search (car c))			(vdwait (car c))			(AutoStop (cadr c))			(vdwait (cadr c))		)	))(defun pp(a)	(play-clip a))(defun play-all() 	(mapcan 'pp clips))(defun show-clips() 	(mapcan  		'((a) 			(print  				(list a (get a 'clip)) 			) 		) 		clips 	) )