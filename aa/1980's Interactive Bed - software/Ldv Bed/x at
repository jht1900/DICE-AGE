(defun exec-loop()	(if grab-on (grab-loop))	(prog (x)		(setq x (xipick))		(if (not (eq x ox))			(print x)		)		(setq ox x)	)	t)(defun time()	(prog (lap count)		(setq lap (tick-count))		(setq count 0)		(while (not (buttonp))			(setq count (+ 1 count))		)		(return (list (- (tick-count) lap)					count				)		)	))(defun tt()	(setq m (wino '((radio fooo rfunc)) 'rad 100 50))	(setq ml (list m)))(defun rfunc(a)	(print a))