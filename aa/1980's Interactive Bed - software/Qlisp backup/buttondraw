(defun go()	(setq paint-wino (wino () 'paint 100 100 5 200))		(setq button-wino (wino button-def 'dir 100 100 110 200))	(setq menu-list (list button-wino))		(setup)	(sh))(setq button-def 	'(	(button up up-func)		(button down down-func)		(button left left-func)		(button right right-func)	))(defun sh()	(show menu-list 'exec-loop))(defun exec-loop()	; do nothing)(setq ydelta 10)(setq xdelta 10)(setq xpos 0)(setq ypos 0)(defun up-func()	(setq ypos (+ ypos ydelta))	(LineTo xpos ypos))(defun down-func()	(setq ypos (- ypos ydelta))	(LineTo xpos ypos))(defun left-func()	(setq xpos (- xpos xdelta))	(LineTo xpos ypos))(defun right-func()	(setq xpos (+ xpos xdelta))	(LineTo xpos ypos))(defun setup()	(ShowPen)	(PenSize xdelta ydelta)	(LineTo 0 0))