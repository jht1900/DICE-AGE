\f2\fs22 \cf0 OSErr FSGetDataForkName (\
   {\field{\*\fldinst{HYPERLINK "file:///Developer/ADC%20Reference%20Library/documentation/Carbon/Reference/File_Manager/Reference/reference.html#//apple_ref/doc/c_ref/HFSUniStr255"}}{\fldrslt \cf2 HFSUniStr255}} * dataForkName\
);\
