// Simple test of displaying QuickTime Movie	img = new Image(320, 240);	mov = new Camera();	mov.output = img;		//mov.pickFile();		//win = new Window(5, 50, 640, 480);	//win.show();	proj = new Projector();	proj.input = img;	proj.output = new Image();	frameCount = 0;	startTime = GetCurrentEventTime();	lastNow = startTime;	processThem();function processThem()	{		mov.process();		proj.process();		frameCount++;		now2 = GetCurrentEventTime();		lapse = now2 - startTime;		fps = frameCount / lapse ;					if (  (now2 - lastNow) > 2.0 )		{			lapse = Math.round( lapse * 100 ) / 100;			fps = Math.round( fps * 100 ) / 100;			//win2.ctl1.title = "Lapse "+lapse+" Frame "+frameCount+" Fps "+fps;			//win2.ctl1.title = " Fps "+fps;
			print(" Fps "+fps);			lastNow = now2;		}	}function spendTime()	{		processThem();	}